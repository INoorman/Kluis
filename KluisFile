#include <Servo.h>

Servo kluisMotor;


void kluisOpen(){
  kluisMotor.attach(9);
  kluisMotor.write(1023);
  
}

void kluisDicht(){
  kluisMotor.attach(9);
  kluisMotor.write(0);
   
}

bool correctCode(int code){

  if(code = 1234){

    kluisOpen();

  }

}


void setup() {
  // put your setup code here, to run once:
  kluisDicht();
 
  
}
